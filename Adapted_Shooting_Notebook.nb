Cell[BoxData[{
 StyleBox[
  RowBox[{
   RowBox[{"wavefunc", "[", 
    RowBox[{"en_", ",", " ", 
     RowBox[{"\[Alpha]_", "?", "NumericQ"}], ",", " ", "\[Lambda]_"}], "]"}], 
   " ", ":=", " ", 
   RowBox[{"NDSolve", "[", 
    RowBox[{
     RowBox[{"{", " ", 
