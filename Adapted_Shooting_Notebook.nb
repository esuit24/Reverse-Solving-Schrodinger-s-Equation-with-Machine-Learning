         RowBox[{"-", "L"}], "]"}], " ", "==", "  ", "0"}], ",", " ", "\n", 
       "\t\t\t\t\t", 
       RowBox[{
        RowBox[{
         RowBox[{"u", "'"}], "[", 
         RowBox[{"-", "L"}], "]"}], " ", "==", " ", "1"}]}], "}"}], ",", " ", 
     "u", ",", " ", 
     RowBox[{"{", 
      RowBox[{"x", ",", " ", 
       RowBox[{"-", "L"}], ",", " ", "0"}], "}"}]}], " ", "]"}]}], 
  "Code"], "\n", 
 StyleBox[
  RowBox[{
   RowBox[{"sol", "[", 
    RowBox[{
     RowBox[{"x_", "?", "NumericQ"}], ",", " ", 
