     RowBox[{"\[Alpha]_", "?", "NumericQ"}], ",", " ", 
     RowBox[{"\[Lambda]_", "?", "NumericQ"}]}], "]"}], " ", ":=", " ", 
   RowBox[{
    RowBox[{"u", "[", "x", "]"}], " ", "/.", 
    RowBox[{
     RowBox[{"wavefunc", "[", 
      RowBox[{"en", ",", " ", "\[Alpha]", ",", " ", "\[Lambda]"}], "]"}], "[", 
     RowBox[{"[", "1", "]"}], "]"}]}]}], "Code"]}], "Code",
